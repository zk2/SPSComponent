{
  "name": "zk2/sps-component",
  "type": "library",
  "description": "SQL/DQL query builder for Doctrine 2",
  "homepage": "https://github.com/zk2/SPSComponent",
  "license": "MIT",
  "keywords": [
    "sql",
    "dql",
    "query builder"
  ],
  "authors": [
    {
      "name": "Evgeniy Budanov",
      "email": "budanov.ua@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1,<8.0",
    "ext-pdo": "*",
    "doctrine/orm": "^2.6",
    "doctrine/dbal": "^2.6",
    "doctrine/annotations": "^1.0",
    "doctrine/cache": "^1.6"
  },
  "require-dev": {
    "ext-json": "*",
    "monolog/monolog" : "^2.1",
    "phpunit/phpunit": "^8.5",
    "symfony/var-dumper": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Zk2\\SpsComponent\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}
