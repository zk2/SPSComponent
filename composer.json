{
  "name": "zk2/sps-component",
  "type": "library",
  "description": "SQL/DQL query builder for Doctrine 2",
  "homepage": "https://github.com/zk2/query-builder",
  "license": "MIT",
  "keywords": [
    "sql",
    "dql",
    "query builder"
  ],
  "authors": [
    {
      "name": "Evgeniy Budanov",
      "email": "budanov.ua@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "doctrine/orm": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*",
    "symfony/var-dumper": "3.3.*",
    "cobaia/monologdoctrine": "1.0.*",
    "squizlabs/php_codesniffer": "^3.0",
    "escapestudios/symfony2-coding-standard": "3.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Zk2\\SpsComponent\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
